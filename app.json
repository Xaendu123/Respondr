{
  "expo": {
    "name": "respondr",
    "slug": "respondr",
    "version": "1.0.0",
    "description": "Activity logging and social platform for first responders",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "respondr",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "deploymentTarget": "15.4",
      "bundleIdentifier": "ch.respondr.app",
      "buildNumber": "1",
      "usesNonExemptEncryption": false,
      "associatedDomains": [
        "webcredentials:respondr.ch"
      ],
      "infoPlist": {
        "CFBundleDisplayName": "Respondr",
        "CFBundleName": "Respondr",
        "NSCameraUsageDescription": "Wir benötigen Zugriff auf die Kamera, um ein Profilbild aufzunehmen",
        "NSPhotoLibraryUsageDescription": "Wir benötigen Zugriff auf deine Fotos, um ein Profilbild auszuwählen",
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "supabase.co": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.2"
            }
          }
        }
      }
    },
    "android": {
      "package": "ch.respondr.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#f97316",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "respondr.ch"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "respondr"
            }
          ]
        }
      ],
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 23
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fff7ed",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#020617"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Wir benötigen Zugriff auf deine Fotos, um ein Profilbild auszuwählen",
          "cameraPermission": "Wir benötigen Zugriff auf die Kamera, um ein Profilbild aufzunehmen"
        }
      ],
      [
        "./plugins/withLocalizedNativePermissions",
        null
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8ac0acb9-0cba-49ff-a36b-3b9f107a93c5"
      }
    },
    "owner": "respondr"
  }
}
